{
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "rules": {
    "quotes": [2, "single"],
    "class-methods-use-this": 0,
    "max-len": ["error", { "code": 120 }],
    "no-underscore-dangle": 0
  },
  "extends": ["airbnb-base", "airbnb-typescript/base"],
  "parserOptions": {
    "project": "./tsconfig.json"
  }
}
